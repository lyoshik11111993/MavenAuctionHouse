<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="340.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.SelectController">
   <children>
      <Pane layoutY="-11.0" prefHeight="100.0" prefWidth="600.0">
         <children>
            <Label layoutX="89.0" layoutY="14.0" prefHeight="17.0" prefWidth="173.0" text="Select a category:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <MenuButton fx:id="selectMenuBtn" layoutX="300.0" layoutY="15.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="150.0" text="Insert Into...">
              <items>
                <MenuItem fx:id="sellerBtn" mnemonicParsing="false" onAction="#showSellers" text="Seller" />
                <MenuItem fx:id="buyerBtn" mnemonicParsing="false" onAction="#showBuyers" text="Buyer" />
                  <MenuItem fx:id="productBtn" mnemonicParsing="false" onAction="#showProducts" text="Product" />
                  <MenuItem fx:id="bidBtn" mnemonicParsing="false" onAction="#showBids" text="Bid" />
              </items>
            </MenuButton>
         </children>
      </Pane>
      <Pane fx:id="workPane" layoutY="92.0" prefHeight="248.0" prefWidth="600.0" />
   </children>
</AnchorPane>
